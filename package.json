{
	"name": "type",
	"version": "0.0.1",
	"description": "real time typing",
	"main": "dist/src/index.js",
	"scripts": {
		"build": "tsc",
		"start": "NODE_ENV=production && node ./dist/src/index.js",
		"dev": "tsx watch --clear-screen=false ./src/index.ts",
		"test:watch": "vitest watch",
		"test": "NODE_ENV=testing vitest --run",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint:check": "eslint .",
		"lint": "eslint --fix ."
	},
	"keywords": [
		"realtime",
		"typing",
		"socket.io",
		"express"
	],
	"author": "wajeht",
	"license": "MIT",
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"express-rate-limit": "^6.11.2",
		"helmet": "^7.1.0",
		"prettier": "^3.2.5",
		"socket.io": "^4.7.5",
		"typescript": "^5.4.5"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "^4.17.21",
		"eslint": "^8.57.0",
		"nodemon": "^3.1.0",
		"supertest": "^6.3.4",
		"tsx": "^4.7.3",
		"vitest": "^0.34.6"
	}
}
