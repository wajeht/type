		"helmet": "^7.0.0",
		"prettier": "^3.0.3",
		"socket.io": "^4.7.2"
	},
	"devDependencies": {
		"eslint": "^8.49.0",
		"nodemon": "^3.0.1",
		"supertest": "^6.3.3",
		"vitest": "^0.34.4"
	}
