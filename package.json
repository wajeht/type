	"description": "real time typing",
	"type": "module",
	"main": "./src/index.js",
	"scripts": {
		"dev": "nodemon ./src/index.js",
		"start": "NODE_ENV=production && node ./src/index.js",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
